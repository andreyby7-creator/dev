{
  "name": "@salespot/api",
  "version": "1.0.0",
  "description": "SaleSpot BY API - NestJS backend",
  "author": "SaleSpot BY Team",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start --port 3001",
    "dev": "nest start --watch --port 3001",
    "start:dev": "nest start --watch --port 3001",
    "start:debug": "nest start --debug --watch --port 3001",
    "start:prod": "node dist/main",
    "lint": "eslint \"src/**/*.ts\" --fix && eslint \"test/**/*.ts\" --fix --config eslint.config.test.js",
    "lint:src": "eslint \"src/**/*.ts\" --fix",
    "lint:test": "eslint \"test/**/*.ts\" --fix --config eslint.config.test.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --no-coverage",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.10.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.15.0",
    "@sentry/node": "^10.10.0",
    "@supabase/supabase-js": "^2.56.0",
    "cache-manager": "^7.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "express": "^5.1.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.7.0",
    "mysql2": "^3.6.0",
    "next-auth": "5.0.0-beta.29",
    "pino": "^9.9.2",
    "prisma": "6.15.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^11.1.6",
    "@swc/core": "^1.13.5",
    "@swc/jest": "^0.2.39",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/node": "^24.3.1",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
