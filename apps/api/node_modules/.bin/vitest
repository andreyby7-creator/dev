#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/boss/Projects/dev/node_modules/.pnpm/vitest@3.2.4_@types+node@24.3.1_@vitest+ui@3.2.4_jiti@2.5.1_msw@2.11.1_@types+node@24.3_2996b8adde913ad8c0088cdb465c1553/node_modules/vitest/node_modules:/home/boss/Projects/dev/node_modules/.pnpm/vitest@3.2.4_@types+node@24.3.1_@vitest+ui@3.2.4_jiti@2.5.1_msw@2.11.1_@types+node@24.3_2996b8adde913ad8c0088cdb465c1553/node_modules:/home/boss/Projects/dev/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/boss/Projects/dev/node_modules/.pnpm/vitest@3.2.4_@types+node@24.3.1_@vitest+ui@3.2.4_jiti@2.5.1_msw@2.11.1_@types+node@24.3_2996b8adde913ad8c0088cdb465c1553/node_modules/vitest/node_modules:/home/boss/Projects/dev/node_modules/.pnpm/vitest@3.2.4_@types+node@24.3.1_@vitest+ui@3.2.4_jiti@2.5.1_msw@2.11.1_@types+node@24.3_2996b8adde913ad8c0088cdb465c1553/node_modules:/home/boss/Projects/dev/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
